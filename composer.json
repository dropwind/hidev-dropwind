{
    "name": "dropwind/hidev-dropwind",
    "type": "yii2-extension",
    "description": "HiDev plugin for DropWind vendor configuration",
    "keywords": [
        "dropwind",
        "hidev",
        "vendor",
        "config"
    ],
    "homepage": "https://github.com/dropwind/hidev-dropwind",
    "license": "proprietary",
    "support": {
        "email": "support@dropwind.com",
        "source": "https://github.com/dropwind/hidev-dropwind",
        "issues": "https://github.com/dropwind/hidev-dropwind/issues",
        "wiki": "https://github.com/dropwind/hidev-dropwind/wiki",
        "forum": "http://forum.hiqdev.com/"
    },
    "authors": [
        {
            "name": "Andrii Vasyliev",
            "role": "Project lead",
            "email": "sol@hiqdev.com",
            "homepage": "http://hipanel.com/"
        }
    ],
    "require": {
        "hiqdev/composer-config-plugin": "*@dev"
    },
    "require-dev": {
        "hiqdev/hidev-php": "<2.0"
    },
    "autoload": {
        "psr-4": {
            "hidev\\dropwind\\": "src"
        }
    },
    "extra": {
        "config-plugin": {
            "hidev": "src/config/hidev.php"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
